template st:nodes {
  "\"nodes\":[\n"
  group {
    "{\"id\": \"" us:get(?s)  "\""
    ",\"label\": \"" str(?s)  "\""
    ",\"group\":" if ( isURI(?s), "\"1\"", if( isBlank(?s), "\"2\"", "\"3\"") )
    "}";
    separator=",\n"
  }
  "\n]\n"
} where {
  bind (us:start() as ?start)
  {select distinct ?s where {
    { ?s ?p ?o } union { ?o ?p ?s }
  } order by ?s}
  bind(us:record(?s) as ?tmp)
}

